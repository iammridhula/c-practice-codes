#include <stdio.h>
int main()
{
    char ch;
    int hra;
    ch=getc(stdin);
    if(ch=='A'){
        printf("45");
    }else if(ch=='B'){
        printf("40");
    }else if(ch=='C'){
        printf("35");
    }else if(ch=='D'){
        printf("30");
    }else{
        printf("Invalid Grade");
    }
}
